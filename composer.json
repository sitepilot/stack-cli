{
    "name": "sitepilot/stack",
    "description": "A light-weight command-line interface for running web applications and additional services using Docker.",
    "keywords": [
        "docker",
        "cli",
        "wordpress",
        "laravel"
    ],
    "homepage": "https://sitepilot.io",
    "type": "project",
    "license": "MIT",
    "support": {
        "source": "https://github.com/sitepilot/stack",
        "issues": "https://github.com/sitepilot/stack/issues"
    },
    "authors": [
        {
            "name": "Nick Jansen",
            "email": "n.jansen@sitepilot.io"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "laravel-zero/framework": "^8.8",
        "laravel-zero/phar-updater": "^1.0.6",
        "illuminate/translation": "^8.59",
        "illuminate/validation": "^8.59",
        "illuminate/view": "^8.58",
        "symfony/process": "^5.3",
        "symfony/yaml": "^5.3"
    },
    "require-dev": {
        "mockery/mockery": "^1.4.3",
        "pestphp/pest": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/Helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": [
        "stack"
    ]
}
